#!/usr/bin/awk -f

BEGIN {
    FS=" " #separation character
	}
{
    if(FILENAME==ARGV[1]){
	if(NR>0){
 	    sub("\015$","",$(NF)) #avoid CR problem
	    
	    status[$1]=$2;	#weise $2 auf $1 zu
	    
	}
    }
    else {
	sub("\015$","",$(NF));

	STAT="0";
	STAT=status[$1];

	
        if(STAT=="LD" || STAT=="DB" || STAT == "P"){
	    print $1" "$2
	}
    }
}



